<html>
    <head>
        <title>File Upload</title>
    </head>
    <body>
        <h1>Upload CSV</h1>
        <form action="/admin" method="POST" enctype="multipart/form-data">
            <input type="file" name="csv">
            <input type="submit" value="Upload File">
        </form>
        <h1>Upload Zip</h1>
         <form action="/admin" method="POST" enctype="multipart/form-data">
            <input type="file" name="zip">
            <input type="submit" value="Upload File">
        </form>
    </body>
</html>
